<%- partial('_partial/bg-cover') %>

<main class="content">
    <div class="container friends-container">
        <div class="card">
            <div class="card-content">
                <!-- artitalk -->
                <script type="text/javascript" src="https://unpkg.com/artitalk">
</script>
                <div id="artitalk_main"></div>
                <script>
                    new Artitalk({
				        appId: '<%- theme.artitalk.appId %>',
				        appKey: '<%- theme.artitalk.appKey %>',
				        atEmoji: '<%- theme.artitalk.atEmoji %>',
				        atComment: '<%- theme.artitalk.atComment %>',
				        lang: '<%- theme.artitalk.lang %>',
						pageSize: '<%- theme.artitalk.pageSize %>',
						bgImg: '<%- theme.artitalk.bgImg %>',
						color1: '<%- theme.artitalk.color1 %>',
						color2: '<%- theme.artitalk.color2 %>',
						color3: '<%- theme.artitalk.color3 %>',
                        // color1: '#6DD0F2',//背景颜色1
                        // color2: '#F59ABE',//背景颜色2，双色
                        // color3: 'white'//字体颜色
                    })
                </script>
                <!-- artitalk.end -->
            </div>
        </div>
    </div>
</main>